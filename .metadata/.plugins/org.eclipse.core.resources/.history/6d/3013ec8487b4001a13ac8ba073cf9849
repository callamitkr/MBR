package com.teast;

import java.util.Arrays;
import java.util.List;

public class ConcurrentHashMapTest {

	public static void main(String[] args) {

		List<String> abc = Arrays.asList("coko","okoc","anangr","anargn") ;
		
		char[] char1 = abc.get(0).toCharArray();
		char[] char2 =abc.get(1).toCharArray();
		
	     Arrays.sort(char1);
	     Arrays.sort(char2);
	     System.out.println(char2);
	     System.out.println(char2);
		System.out.println(char1 ==(char2));
		

	}

}
